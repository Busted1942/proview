{
  "name": "proview-suite-runner",
  "version": "0.3",
  "private": true,
  "type": "module",
  "scripts": {
  "test": "node runner.js",
  "test:id": "node runner.js --id",
  "test:smoke": "cross-env INCLUDE_TAGS=smoke node runner.js",
  "test:installer": "cross-env INCLUDE_TAGS=installer node runner.js",
  "test:grep": "cross-env GREP_TITLE=escalation node runner.js",
  "test:mini": "cross-env FORCE_MODEL=gpt-4o-mini node runner.js",
  "test:only-mini": "cross-env INCLUDE_MODEL=gpt-4o-mini node runner.js"
},
  "dependencies": {
    "js-yaml": "^4.1.0",
    "dotenv": "^16.4.5",
    "openai": "^4.56.0",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.17"
  }
}
